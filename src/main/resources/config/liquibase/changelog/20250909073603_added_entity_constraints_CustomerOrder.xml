    <!--
        Added the constraints for entity CustomerOrder.
    -->
    <changeSet id="20250909073603-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="customer_order"
                                 constraintName="fk_customer_order__customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"
                                 />

        <addForeignKeyConstraint baseColumnNames="sales_by_id"
                                 baseTableName="customer_order"
                                 constraintName="fk_customer_order__sales_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"
                                 />
    </changeSet>
</databaseChangeLog>
